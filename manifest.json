{
    "manifest_version": 3,
    "name": "Google Colab Question Mark Begone",
    "description": "Remove those pesky question mark boxes that make colab help pages hard to read when using the R language.",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://colab.research.google.com/*"
            ]
        }
    ],
    "icons" : {
        "128": "icons/icon128.png",
        "48": "icons/icon48.png",
        "32": "icons/icon32.png",
        "16": "icons/icon16.png"
    }
}